<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.imei.app.dao.DiaryTypeDao">
	<select id="queryAllDiaryTypes" resultType="DiaryType">
		SELECT
		id,
		name,
		sort_order
		FROM
		diarytype
		ORDER BY
		sort_order ASC
	</select>
</mapper>
